{
	"lock": false,
	"tasks": {
		"check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
		"cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable -A -",
		"manifest": "deno task cli manifest $(pwd)",
		"dev": "deno run -A --unstable-kv --watch=src/static/,src/routes/ src/dev.ts",
		"build": "deno run -A --unstable-kv src/dev.ts build",
		"docker:build": "docker build -t system-nvll .",
		"deploy": "deno run -A --unstable-kv src/dev.ts build && deployctl deploy --prod --project=system-nvll",
		"preview": "deno run -A --unstable-kv src/main.ts",
		"update": "deno run -A -r https://fresh.deno.dev/update ."
	},
	"lint": {
		"include": ["**/*.ts", "**/*.tsx"],
		"exclude": ["**/_fresh/*", "**/*.hbs"],
		"rules": {
			"tags": ["recommended", "fresh"],
			"include": ["no-unused-vars", "no-var-requires", "no-explicit-any", "no-extraneous-class", "no-non-null-assertion", "ban-untagged-todo"],
			"exclude": []
		}
	},
	"fmt": {
		"useTabs": true,
		"lineWidth": 155,
		"indentWidth": 4,
		"semiColons": false,
		"singleQuote": true,
		"proseWrap": "preserve",
		"include": ["**/*.ts", "**/*.tsx"],
		"exclude": ["**/_fresh/*", "**/*.hbs"]
	},
	"exclude": ["**/_fresh/*"],
	"imports": {
		"$fresh/": "https://deno.land/x/fresh@1.6.8/",
		"$std/": "https://deno.land/std@0.224.0/",
		"@noble/ciphers/": "https://esm.sh/@noble/ciphers@0.5.3/",
		"@noble/curves/": "https://esm.sh/@noble/curves@1.4.2/",
		"@noble/hashes/": "https://esm.sh/@noble/hashes@1.4.0/",
		"@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
		"@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
		"@tailwindcss/forms": "npm:/@tailwindcss/forms@^0.5.7",
		"djwt": "https://deno.land/x/djwt@v3.0.2/mod.ts",
		"handlebars": "https://esm.sh/handlebars@4.7.8",
		"nodemailer": "https://esm.sh/nodemailer@6.9.14",
		"nodemailer/": "https://esm.sh/nodemailer@6.9.14/",
		"preact": "https://esm.sh/preact@10.19.6",
		"preact/": "https://esm.sh/preact@10.19.6/",
		"saslprep": "npm:/saslprep@^1.0.3",
		"tailwindcss": "https://esm.sh/tailwindcss@3.4.7",
		"tailwindcss/plugin": "https://esm.sh/tailwindcss@3.4.7/plugin.js",
		"ui7": "https://deno.land/x/ui7@v0.2.3/mod.ts",
		"ulid": "https://deno.land/x/ulid@v0.3.0/mod.ts",
		"zod": "https://deno.land/x/zod@v3.23.8/mod.ts"
	},
	"compilerOptions": { "types": ["./definition.d.ts"], "jsx": "react-jsx", "jsxImportSource": "preact" },
	"nodeModulesDir": false,
	"deploy": { "project": "c86c6599-942c-486e-9e40-84931bf51e86", "exclude": ["**/node_modules"], "include": [], "entrypoint": "src\\main.ts" }
